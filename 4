float cl,dl,in3,gal,l,ml,m3,yd3;
void convert_cl()
{
    dl=cl*0.1;
    in3=cl*0.61023744094;
    gal=cl*0.002199736;
    l=cl*0.01;
    m3=cl*0.00001;
    ml=cl*10;
    yd3=cl*0.0000130794;
}
void convert_dl()
{
    cl=dl*10;
    in3=dl*6.1023744094;
    gal=dl*0.0219973603;
    l=dl/10;
    ml=dl*100;
    m3=dl/10000;
    yd3=dl*0.000130794182;
}
void convert_in3()
{
    cl=in3/0.61023744094;
    dl=in3/6.1023744094;
    gal=in3*0.00360472151;
    l=in3*0.016387064;
    ml=in3*16.387064;
    m3=in3*0.000016387064;
    yd3=in3*0.000021433326;
}
void convert_gal()
{
