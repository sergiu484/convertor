            cin>>m2;
            convert_m2();
            cout<<"Valoarea in ari este: "<<a<<"\n"<<"Valoarea in acri la a 2 este: "<<ac<<"\n"<<"Valoarea in centimetri la a 2  este: "<<cm2<<"\n"<<"Valoarea in picioare la a 2 este: "<<feet2<<"\n"<<"Valoarea in hectare: "<<ha<<"\n"<<"Valoarea in inch la a 2 este: "<<in2<<"\n"<<"Valoare in kilometri la a2 este: "<<km2<<"\n"<<"Valoarea in milimetri la a 2 este: "<<mm2<<"\n"<<"Valoarea in yard la a 2 este: "<<yd2<<endl;
        }
        else if(n==9)
        {
            cout<<"Alege valoare pentru a fi convertata: "<<endl;
            cin>>mm2;
            convert_mm2();
            cout<<"Valoarea in ari este: "<<a<<"\n"<<"Valoarea in acri la a 2 este: "<<ac<<"\n"<<"Valoarea in centimetri la a 2  este: "<<cm2<<"\n"<<"Valoarea in picioare la a 2 este: "<<feet2<<"\n"<<"Valoarea in hectare: "<<ha<<"\n"<<"Valoarea in inch la a 2 este: "<<in2<<"\n"<<"Valoare in metri la a2 este: "<<m2<<"\n"<<"Valoarea in kilometri la a 2 este: "<<km2<<"\n"<<"Valoarea in yard la a 2 este: "<<yd2<<endl;
        }
        else if(n==10)
        {
            cout<<"Alege valoare pentru a fi convertata: "<<endl;
            cin>>yd2;
            convert_yd2();
            cout<<"Valoarea in ari este: "<<a<<"\n"<<"Valoarea in acri la a 2 este: "<<ac<<"\n"<<"Valoarea in centimetri la a 2  este: "<<cm2<<"\n"<<"Valoarea in picioare la a 2 este: "<<feet2<<"\n"<<"Valoarea in hectare: "<<ha<<"\n"<<"Valoarea in inch la a 2 este: "<<in2<<"\n"<<"Valoare in metri la a2 este: "<<m2<<"\n"<<"Valoarea in milimetri la a 2 este: "<<mm2<<"\n"<<"Valoarea in kilometri la a 2 este: "<<km2<<endl;
        }
    }
    else if (x==4)
    {
        cout<<"Alege 1 pentru a converta centilitri. "<<"\n"<<"Alege 2 pentru a converta decilitri."<<"\n"<<"Alege 3 pentru a converta inch cubi."<<"\n"<<"Alege 4 pentru a converta galoane."<<"\n"<<"Alege 5 pentru a converta litri. "<<"\n"<<"Alege 6 pentru a converta mililitri."<<"\n"<<"Alege 7 pentru a converta metri cubi. "<<"\n"<<"Alege 8 pentru a converta yarzi cubi. "<<endl;
        cin>>n;
        if(n==1)
        {
            cout << "Alege volumul in centilitri: "<<endl;
            cin>>cl;
            convert_cl();
            cout<<"Volumul in decilitri este: "<<dl<<"\n"<<"Volumul in galoane este: "<<gal<<"\n"<<"Volumul in inch cubi este: "<<in3<<"\n"<<"Volumul in litri este: "<<l<<"\n"<<"Volumul in metri cubi este: "<<m3<<"\n"<<"Volumul in mililitri este: "<<ml<<"\n"<<"Volumul in yarzi cubi este: "<<yd3<<endl;
        }
        if(n==2)
        {
            cout << "Alege volumul in decilitri: "<<endl;
            cin>>dl;
            convert_dl();
            cout<<"Volumul in centilitri este: "<<cl<<"\n"<<"Volumul in galoane este: "<<gal<<"\n"<<"Volumul in inch cubi este: "<<in3<<"\n"<<"Volumul in litri este: "<<l<<"\n"<<"Volumul in metri cubi este: "<<m3<<"\n"<<"Volumul in mililitri este: "<<ml<<"\n"<<"Volumul in yarzi cubi este: "<<yd3<<endl;
        }
        if(n==3)
        {
            cout << "Alege volumul in inch cubi: "<<endl;
            cin>>in3;
            convert_in3();
            cout<<"Volumul in centilitri este: "<<cl<<"\n"<<"Volumul in decilitri este: "<<dl<<"\n"<<"Volumul in galoane este: "<<gal<<"\n"<<"Volumul in litri este: "<<l<<"\n"<<"Volumul in metri cubi este: "<<m3<<"\n"<<"Volumul in mililitri este: "<<ml<<"\n"<<"Volumul in yarzi cubi este: "<<yd3<<endl;
        }
        if(n==4)
        {
            cout << "Alege volumul in galoane: "<<endl;
            cin>>gal;
            convert_gal();
            cout<<"Volumul in centilitri este: "<<cl<<"\n"<<"Volumul in decilitri este: "<<dl<<"\n"<<"Volumul in inch cubi este: "<<in3<<"\n"<<"Volumul in litri este: "<<l<<"\n"<<"Volumul in metri cubi este: "<<m3<<"\n"<<"Volumul in mililitri este: "<<ml<<"\n"<<"Volumul in yarzi cubi este: "<<yd3<<endl;
        }
        if(n==5)
        {
            cout << "Alege volumul in litri: "<<endl;
            cin>>l;
            convert_l();
            cout<<"Volumul in centilitri este: "<<cl<<"\n"<<"Volumul in decilitri este: "<<dl<<"\n"<<"Volumul in inch cubi este: "<<in3<<"\n"<<"Volumul in galoane este: "<<gal<<"\n"<<"Volumul in metri cubi este: "<<m3<<"\n"<<"Volumul in mililitri este: "<<ml<<"\n"<<"Volumul in yarzi cubi este: "<<yd3<<endl;
        }
        if(n==6)
        {
            cout << "Alege volumul in mililitri: "<<endl;
            cin>>ml;
            convert_ml();
            cout<<"Volumul in centilitri este: "<<cl<<"\n"<<"Volumul in decilitri este: "<<dl<<"\n"<<"Volumul in inch cubi este: "<<in3<<"\n"<<"Volumul in galoane este: "<<gal<<"\n"<<"Volumul in metri cubi este: "<<m3<<"\n"<<"Volumul in litri este: "<<l<<"\n"<<"Volumul in yarzi cubi este: "<<yd3<<endl;
        }
        if(n==7)
        {
            cout << "Alege volumul in metri cubi: "<<endl;
            cin>>m3;
            convert_m3();
            cout<<"Volumul in centilitri este: "<<cl<<"\n"<<"Volumul in decilitri este: "<<dl<<"\n"<<"Volumul in inch cubi este: "<<in3<<"\n"<<"Volumul in galoane este: "<<gal<<"\n"<<"Volumul in litri este: "<<l<<"\n"<<"Volumul in mililitri este: "<<ml<<"\n"<<"Volumul in yarzi cubi este: "<<yd3<<endl;
        }
        if(n==8)
        {
            cout << "Alege volumul in yarzi cubi: "<<endl;
            cin>>yd3;
            convert_yd3();
            cout<<"Volumul in centilitri este: "<<cl<<"\n"<<"Volumul in decilitri este: "<<dl<<"\n"<<"Volumul in inch cubi este: "<<in3<<"\n"<<"Volumul in galoane este: "<<gal<<"\n"<<"Volumul in metri cubi este: "<<m3<<"\n"<<"Volumul in litri este: "<<l<<"\n"<<"Volumul in mililitri este: "<<ml<<endl;
        }
    }
    return 0;
}
