void convert_cm()
{
    feet=cm/ 30.48;
    inch=cm/ 2.54;
    km=cm/10000;
    m=cm/100;
    mi=cm/160934.4;
    mm=cm/0.1;
    nmi=cm*0.000005399;
    yard=cm/91.44;
}
void convert_feet()
{
    cm=feet*30.48;
    inch=feet* 12;
    km=feet*0.0003048;
    m=feet*0.3048;
    mi=feet/5280;
    mm=feet*304.8;
    nmi=feet/ 6076.11549;
    yard=feet/3;
}
void convert_inch()
{
    cm=inch* 2.54;
    feet=inch/12;
    km=inch* 0.0000254;
    m=inch*0.0254;
    mi=inch/ 63360;
    mm=inch*25.4;
    nmi=inch/ 72913.3858;
    yard=inch/36;
}
void  convert_km()
{
    cm= km*100000;
    feet=km/0.0003048;
    inch=km/ 0.0000254;
    m=km*1000;
    mi=km/ 1.609344;
    mm=km*1000000;
    nmi=km/1.852;
    yard=km/ 0.0009144;
}
void convert_m()
{
    cm=m*100;
    feet=m /0.3048;
    inch =m/ 0.0254;
    km=m/1000;
    mi=m/ 1609.344;
    mm=m*1000;
    nmi=m/1852;
    yard=m/0.9144;
}
void convert_mi()
{
    cm=mi* 160934.4;
    feet=mi* 5280;
    inch=mi* 63360;
    km=mi * 1.609344;
    m=mi* 1609.344;
    mm=mi* 1609344;
    nmi=mi/ 1.15078;
    yard=mi* 1760;
}
void convert_mm()
{
    cm=mm/10;
