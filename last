void convert_yard()
{
    feet=yard*3;
    cm=yard*91.44;
    inch=yard*36;
    km=yard* 0.0009144;
    mi=yard/ 1760;
    mm=yard* 914.4;
    m=yard*0.9144;
    nmi=yard/2025.37183;
}
void convert_Celsius()
{
    fahrenheit = (celsius * 9 / 5) + 32;
    kelvin=celsius+273.15;
    rankine=(celsius+273.15)*9/5;
}
void convert_Fahrenheit()
{
    celsius = (fahrenheit - 32) * 5 / 9;
    kelvin = (fahrenheit +459.67) *5/9;
    rankine = fahrenheit +459.67;
}
void  convert_Kelvin()
{
    celsius=kelvin-273.15;
    fahrenheit=kelvin*9/5 - 459.67;
    rankine=kelvin*9/5;
}
void convert_Rankine()
{
    celsius=(rankine-491.67)* 5/9;
    kelvin=rankine *5/9;
    fahrenheit=rankine-459.67;
}
int main()
{
    char k[10],x[15],n[10];
    cout<<"Alege A pentru limba romana."<<"\n"<<"Choose B for English language ."<<endl;
    cin>>k;
    while(k[0]<=64 || k[0]>=67)
    {
        cout<<"Da-ti alta comanda: ";
        cin>>k;
    }
    if(k[0]==65)
    {
        cout<<"Alege A pentru a converta temperatura."<<"\n"<<"Alege B pentru a converta lungimea."<<"\n"<<"Alege C pentru a converta aria."<<"\n"<<"Alege D pentru a converta volum."<<"\n"<<"Alege E pentru a converta viteza."<<"\n"<<"Alege F pentru a converta greutate."<<"\n"<<"Alege G pentru a converta presiune."<<"\n"<<"Alege H pentru a converta timp."<<"\n"<<"Alege I pentru a converta energie."<<"\n"<<"Alege J pentru a converta densitate"<<"\n"<<"Alege K pentru a converta consum de combustibil."<<endl;
        cin>>x;
        cout<<fixed<<setprecision(20);
        while(x[0]<=64 || x[0]>=76)
        {
            cout<<"Da-ti alta comanda: ";
            cin>>x;
        }
        if(x[0]==65)
        {
            cout<<"Alege A pentru a converta grade Celsius." <<"\n"<<"Alege B pentru a converta grade Fahrenheit." <<"\n"<<"Alege C pentru a converta grade Kelvin."<<"\n"<<"Alege D pentru a converta grade Rankine."<<endl;
            cin >>n;
            while(n[0]<=64 ||n[0]>=69)
            {
                cout<<"Da-ti alta comanda: ";
                cin>>n;
            }
            if(n[0]==65)
            {
                cout << "Alege temperatura in Celsius: ";
                cin >> celsius;
                convert_Celsius();
                cout << "Temperatura in Fahrenheit este: " << fahrenheit<<"\n"<< "Temperatura in Kelvin este: "<< kelvin<<"\n"<< "Temperatura in Rankine este: " << rankine<<endl;
            }
            else if(n[0]==66)
            {
                cout<<"Alege tempetatura in Fahrenheit: ";
                cin>>fahrenheit;
                convert_Fahrenheit();
                cout << "Temperatura in Celsius este: " << celsius<<" \n"<<"Temperatura in Kelvin este: "<<kelvin<<"\n<<"<<"Temperatura in Rankine este: "<<rankine<<endl;
            }
            else if(n[0]==67)
            {
                cout<<"Alege tempetatura in Kelvin: ";
                cin>>kelvin;
                convert_Kelvin();
                cout<<"Temperatura in Celsius este: "<<celsius<<"\n"<<"Temperatura in Fahrenheit este: "<<fahrenheit<<"\n"<<"Temperatura in Rankine este: "<<rankine<<endl;
            }
            else if(n[0]==68)
            {
                cout<<"Alege tempetatura in Rankine: ";
                cin>>rankine;
                convert_Rankine();
                cout<<"Temperatura in Celsius este: "<<celsius<<"\n"<<"Temperatura in Kelvin este: "<<kelvin<<"\n"<<"Temperatura in Fahrenheit este: "<<fahrenheit<<endl;
            }
        }

        else if(x[0]==66)
        {
            cout <<"Alege A pentru a converta centimetri. "<<"\n"<<"Alege B pentru a converta picioare. "<<"\n"<<"Alege C pentru a converta inch."<<"\n"<<"Alege D pentru a converta kilometri."<<"\n"<<"Alege E pentru a converta metri."<<"\n"<<"Alege F pentru a converta mile."<<"\n"<<"Alege G pentru a converta milimetri."<<"\n"<<"Alege H pentru a converta mile maritime. "<<"\n"<<"Alege I pentru a converta yard." <<endl;
            cin>>n;
            while(n[0]<=64 || n[0]>=74)
            {
                cout<<"Da-ti alta comanda: ";
                cin>>n;
            }
            if(n[0]==65)
            {
                cout << "Alege lungimea in centimetri: ";
                cin >> cm;
                convert_cm();
                cout<<"Lungimea in picioare este: "<<feet<<"\n"<<"Lungimea in inch este: "<<inch<<"\n"<<"Lungimea in kilometri este: "<<km<<"\n"<<"Lungimea in metri este: "<<m<<"\n"<<"Lungimea in mile este: "<<mi<<"\n"<<"Lungimea in milimetri este: "<<mm<<"\n"<<"Lungimea in mile maritime este: "<<nmi<<"\n"<<"Lungimea in yard este: "<<yard<<endl;
            }
            else if(n[0]==66)
            {
                cout<<"Alege lungimea in picioare: ";
                cin>>feet;
                convert_feet();
                cout<<"Lungimea in centimetri este: "<<cm<<"\n"<<"Lungimea in inch este: "<<inch<<"\n"<<"Lungimea in kilometri este: "<<km<<"\n"<<"Lungimea in metri este: "<<m<<"\n"<<"Lungimea in mile este: "<<mi<<"\n"<<"Lungimea in milimetri este: "<<mm<<"\n"<<"Lungimea in mile maritime este: "<<nmi<<"\n"<<"Lungimea in yard este: "<<yard<<endl;
            }
            else if(n[0]==67)
            {
                cout<<"Alege lungimea in inch: ";
                cin>>inch;
                convert_inch();
                cout<<"Lungimea in centimetri este: "<<cm<<"\n"<<"Lungimea in picioare este: "<<feet<<"\n"<<"Lungimea in kilometri este: "<<km<<"\n"<<"Lungimea in metri este: "<<m<<"\n"<<"Lungimea in mile este: "<<mi<<"\n"<<"Lungimea in milimetri este: "<<mm<<"\n"<<"Lungimea in mile maritime este: "<<nmi<<"\n"<<"Lungimea in yard este: "<<yard<<endl;
            }
            else if(n[0]==68)
            {
                cout<<"Alege lungimea in kilometri: ";
                cin>>km;
                convert_km();
                cout<<"Lungimea in centimetri este: "<<cm<<"\n"<<"Lungimea in picioare este: "<<feet<<"\n"<<"Lungimea in inch este: "<<inch<<"\n"<<"Lungimea in metri este: "<<m<<"\n"<<"Lungimea in mile este: "<<mi<<"\n"<<"Lungimea in milimetri este: "<<mm<<"\n"<<"Lungimea in mile maritime este: "<<nmi<<"\n"<<"Lungimea in yard este: "<<yard<<endl;
            }
            else if(n[0]==69)
            {
                cout<<"Alege lungimea in metri: ";
                cin>>m;
                convert_m();
                cout<<"Lungimea in centimetri este: "<<cm<<"\n"<<"Lungimea in picioare este: "<<feet<<"\n"<<"Lungimea in inch este: "<<inch<<"\n"<<"Lungimea in kilometri este: "<<km<<"\n"<<"Lungimea in mile este: "<<mi<<"\n"<<"Lungimea in milimetri este: "<<mm<<"\n"<<"Lungimea in mile maritime este: "<<nmi<<"\n"<<"Lungimea in yard este: "<<yard<<endl;
            }
