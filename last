                cout<<"Milligrams/meters^3 converted in grams/meters^3 is: "<<gm3<<
                    "\n"<<"Milligrams/meters^3 converted in kilograms/meters^3 is: "<<kgm3<<
                    "\n"<<"Milligrams/meters^3 converted in tonnes/meters^3 is: "<<tm3<<endl;
            }
            else if(n[0]==66)
            {
                cout<<"Choose the amount to be converted: "<<endl;
                cin>>gm3;
                convert_gm3();
                cout<<"Grams/meters^3 converted in milligrams/meters^3 is: "<<mgm3<<
                    "\n"<<"Grams/meters^3 converted in kilograms/meters^3 is: "<<kgm3<<
                    "\n"<<"Grams/meters^3 converted in tonnes/meters^3 is: "<<tm3<<endl;
            }
            else if(n[0]==67)
            {
                cout<<"Choose the amount to be converted: "<<endl;
                cin>>kgm3;
                convert_kgm3();
                cout<<"Kilograms/meters^3 converted in grams/meters^3 is: "<<gm3<<
                    "\n"<<"Kilograms/meters^3 converted in milligrams/meters^3 is: "<<mgm3<<
                    "\n"<<"Kilograms/meters^3 converted in tonnes/meters^3 is: "<<tm3<<endl;
            }
            else if(n[0]==68)
            {
                cout<<"Choose the amount to be converted: "<<endl;
                cin>>tm3;
                convert_tm3();
                cout<<"Tonnes/meters^3 converted in grams/meters^3 is: "<<gm3<<
                    "\n"<<"Tonne/meters^3 converted in milligrams/meters^3 is: "<<mgm3<<
                    "\n"<<"Tonnes/meters^3 converted in kilograms/meters^3 is: "<<kgm3<<endl;
            }
        }
        else if(x[0]==75)
        {
            cout << "Choose A to convert Liters/100 kilometers."<<
                 "\n"<<"Choose B to convert Mile/gallon." <<
                 "\n"<<"Choose D to convert Mile/liters."<< endl;
            cin>>n;
            while(n[0]<=64 || n[0]>=68 )
            {
                cout<<"Choose another command: ";
                cin>>n;
            }
            if(n[0]==65)
            {
                cout<<"Choose the amount to be converted: "<<endl;
                cin>>L100km;
                convert_L100_km();
                cout<<"Liters/100 kilometers converted in Mile/gallon is: "<<milegalon<<
                    "\n"<<"Liters/100 kilometers converted in Mile/liters is: "<<mileL<<endl;
            }
            else if(n[0]==66)
            {
                cout<<"Choose the amount to be converted: "<<endl;
                cin>>milegalon;
                convert_mile_galon();
                cout<<"Mile/gallon converted in Liters/100 kilometers: "<<L100km<<
                    "\n"<<"Mile/gallon converted in Mile/liters is: "<<mileL<<endl;
            }
            else if(n[0]==67)
            {
                cout<<"Choose the amount to be converted: "<<endl;
                cin>>mileL;
                convert_mile_L();
                cout<<"Mile/Liters converted in Liters/100 kilometers is: "<<L100km<<
                    "\n"<<"Mile/Liters converted in Mile/gallon is: "<<milegalon<<endl;
            }

        }

    }
    return 0;
}
