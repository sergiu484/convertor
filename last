float L100km, milegalon, mileL;
void convert_L100_km()
{
    milegalon=L100km*235.2;
    mileL=L100km*62.14;
}
void convert_mile_galon()
{
    L100km=milegalon/235.2;
    mileL=milegalon*0.2642;
}
void convert_mile_L()
{
    L100km=mileL/62.14;
    milegalon=mileL/0.2642;
}
void convert_tm3()
{
    mgm3=tm3*1000000000;
    gm3=tm3*1000000;
    kgm3=tm3*1000;
}
void convert_kgm3()
{
    mgm3=kgm3*1000000;
    gm3=kgm3*1000;
    tm3=kgm3*0.001;
}
