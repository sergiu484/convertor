{
    mgm3=gm3*1000;
    kgm3=gm3*0.001;
    tm3=gm3*0.000001;
}
void convert_mgm3()
{
    gm3=mgm3*0.001;
    kgm3=mgm3*0.000001;
    tm3=mgm3/1000000000;
}
void convert_joule()
{
    kj=j*0.001;
    kwh=j*2.777777777;
    wh=j*0.00027777777;
}
void convert_kilojoule()
{
    j=kj/0.001;
    kwh=kj*3600;
    wh=kj*1000;
}
void convert_kilowatt_ora()
{
    j=kwh/2.777777777;
    kj=kwh/3600;
    wh=kwh*0.001;
}
void convert_watt_ora()
{
    j=wh/0.00027777777;
    kj=wh/1000;
    kwh=wh*1000;
}
